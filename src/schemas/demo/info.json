        [
          { "label": "姓名", "value": "index.name", "size": "half-xs" },
          { "label": "性别", "value": "index.gender", "size": "xs", "form": { "mode": "pop", "name": "radio" } },
          { "label": "出生年月", "value": "index.birthdate", "form": { "name": "date" }, "size": "half-label" }
        ],
        [
          { "label": "公民身份号码", "value": "index.idNo" },
          { "label": "血型", "value": "content.bloodType", "form": { "mode": "pop", "name": "radio", "options": "血型" }, "size": "half-label" }
        ],
        [
          { "label": "医疗账号", "value": "" },
          { "label": "民族", "value": "", "form": { "name": "select" }, "size": "half-label" }
        ],
        [
          { "label": "证件类型", "value": "", "form": { "name": "select" } },
          { "label": "籍贯", "value": "", "form": { "name": "cascader" } }
        ],
        [
          { "label": "证件编号", "value": "" },
          { "label": "出生地", "value": "", "form": { "name": "cascader" } }
        ]
      ]
    }
  ]
}
