            "form": { "name": "picture" },
            "style": {
              "position": "absolute",
              "right": 0,
              "height": "calc(var(--row-height) * 3)",
              "width": "var(--label-width)",
              "zIndex": "999"
            },
            "styles": {
              "label": { "display": "none" },
              "value": { "width": "100%", "position": "absolute", "right": 0, "padding": "2px" }
